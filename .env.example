# Example Environment Variables
# Copy this file to .env for local development

# Development mode settings
# VITE_DEV_MODE is redundant - Vite automatically sets DEV/PROD based on command
VITE_ALLOW_DESKTOP_DEV=true

# API endpoints
# VITE_API_URL=your_api_url_here

# Feature flags
# VITE_FEATURE_ANALYTICS=false
# VITE_FEATURE_DEBUG=true

# Google Analytics Configuration
# VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Coinbase CDP Authentication Configuration
VITE_COINBASE_PROJECT_ID=your_coinbase_project_id_here

VITE_ASSET='usdc'

# Development Test Address
VITE_DEV_ADDRESS=0xd955F5Cdee0DE471c64Ab648E1878804ba5FEA12

# Yellow WebSocket URL
# VITE_YELLOW_WS_URL=wss://clearnet.yellow.com/ws